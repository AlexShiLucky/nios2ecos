# --------------------------------------------------------------------------
# Nios II targets

package CYGPKG_HAL_NIOS2 {
	alias		{ "Nios II common HAL"}
	directory	hal/nios2/arch/
	script		hal_nios2.cdl
	hardware
	description "
    	    The Nios II architecture HAL package provides generic support
    	    for this processor architecture. It is also necessary to
    	    select a specific target platform HAL package."
}


target nios2_dev_board {
        alias		{ "Nios II Development Board (Altera)" }
	packages        { CYGPKG_HAL_NIOS2
                          CYGPKG_HAL_SOPC
                          CYGPKG_ALTERA_AVALON_UART_COMM
                          CYGPKG_ALTERA_AVALON_JTAG_UART_COMM
                          CYGPKG_ALTERA_AVALON_UART
                          CYGPKG_ALTERA_AVALON_JTAG_UART
                          CYGPKG_ALTERA_AVALON_TIMER
		          CYGPKG_DEVS_ETH_SMSC_LAN91CXX
                          CYGPKG_ALTERA_AVALON_LAN91C111
                          CYGPKG_ALTERA_AVALON_LCD_16207
                          CYGPKG_ALTERA_AVALON_CF
                          CYGPKG_ALTERA_AVALON_AM29LV065D_FLASH
                          CYGPKG_DEVS_FLASH_AMD_AM29XXXXX
        }
        description "
            This target provides the packages needed to run
            eCos on a Nios II. In addition, components are included
            which support the flash devices found on the Altera
            Nios II development boards."
}

target nios2 {
        alias		{ "Nios II Generic" }
	packages        { CYGPKG_HAL_NIOS2
                          CYGPKG_HAL_SOPC
                          CYGPKG_ALTERA_AVALON_UART_COMM
                          CYGPKG_ALTERA_AVALON_JTAG_UART_COMM
                          CYGPKG_ALTERA_AVALON_UART
                          CYGPKG_ALTERA_AVALON_JTAG_UART
                          CYGPKG_ALTERA_AVALON_TIMER
		          CYGPKG_DEVS_ETH_SMSC_LAN91CXX
                          CYGPKG_ALTERA_AVALON_LAN91C111
                          CYGPKG_ALTERA_AVALON_LCD_16207
                          CYGPKG_ALTERA_AVALON_CF
        }
        description "
            This target provides the packages needed to run
            eCos on any Nios II system."
}


target nios2_neek {
        alias		{ "Base NEEK board" }
	packages        { CYGPKG_HAL_NIOS2
                          CYGPKG_HAL_SOPC
                          CYGPKG_ALTERA_AVALON_UART_COMM
                          CYGPKG_ALTERA_AVALON_JTAG_UART_COMM
                          CYGPKG_ALTERA_AVALON_UART
                          CYGPKG_ALTERA_AVALON_JTAG_UART
                          CYGPKG_ALTERA_AVALON_TIMER                                                            
        }
        description "
            Base NEEK Board"
}


package CYGPKG_DEVS_FLASH_NEEK {
	alias 		{ "FLASH memory support for Altera NEEK board" flash_neek }
	directory	devs/flash/neek
	script		flash_neek.cdl
	hardware
        description "
           This package contains hardware support for FLASH memory
	   on the Altera NEEK platform."
}


package CYGPKG_IO_EPCS {
    alias         { "EPCS device drivers" }
    hardware
    directory     io/epcs
    script        io_epcs.cdl
    description "This package provides a driver for the SPI unit of the EPCS
                 controller family."
}

